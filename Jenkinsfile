pipeline {
  agent any 


  stages {
    stage('Build') {
      steps {
        echo 'Our application starts to build'
      }
    }
    stage('Prepare') {
      steps {
        echo 'our application starts to prepare for test'
      }
    }
    stage('Test') {
      steps {
        echo 'our application is going through the test'
      }
    }
    stage('Deploy'){
      steps {
        echo 'our app finally ready to deploy'
      }
    }
  }
}
